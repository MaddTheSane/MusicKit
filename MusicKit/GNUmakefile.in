CONFIG = OTHER_CFLAGS="@DEFS@" CONFIGURED_LIBS="@LIBS@" SGMLTRANS="@SGMLTRANS@"

# palettes utilities examples applications
all: frameworks 

frameworks:
	$(MAKE) -C Frameworks

palettes:
	$(MAKE) -C Palettes

utilities:
	$(MAKE) -C Utilities

examples:
	$(MAKE) -C Examples

applications:
	$(MAKE) -C Applications

documentation:
	$(MAKE) -C Documentation
